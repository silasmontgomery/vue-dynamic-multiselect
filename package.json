{
  "name": "vue-dynamic-multiselect",
  "version": "1.0.4",
  "description": "A VueJS plugin that provides a searchable and reactive multiselect list component with no dependencies.",
  "main": "dist/vue-dynamic-multiselect.umd.js",
  "module": "dist/vue-dynamic-multiselect.esm.js",
  "unpkg": "dist/vue-dynamic-multiselect.min.js",
  "browser": {
    "./sfc": "src/vue-dynamic-multiselect.vue"
  },
  "scripts": {
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-dynamic-multiselect.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-dynamic-multiselect.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-dynamic-multiselect.min.js"
  },
  "devDependencies": {
    "parse5": "^2.2.3",
    "rollup": "^0.57.1",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-vue": "^4.7.2",
    "vue": "^2.5.7",
    "vue-template-compiler": "^2.5.21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/silasmontgomery/vue-dynamic-multiselect.git"
  },
  "keywords": [
    "vue",
    "js",
    "vuejs",
    "javascript",
    "component",
    "select",
    "multiselect",
    "multi",
    "multiple",
    "list",
    "ajax",
    "reactive",
    "search",
    "searchable",
    "plugin",
    "dynamic"
  ],
  "author": "Silas Montgomery",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/silasmontgomery/vue-dynamic-multiselect/issues"
  },
  "homepage": "https://github.com/silasmontgomery/vue-dynamic-multiselect#readme"
}
